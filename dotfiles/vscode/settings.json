{
    "terminal.integrated.fontFamily": "Inconsolata for Powerline",
    "gitlens.currentLine.enabled": false,
    "gitlens.blame.format": "${author}: ${message|40?} ${agoOrDate|14-}",
    "solidity.compileUsingRemoteVersion": "v0.6.12+commit.27d51765",
    "files.autoSave": "onFocusChange",
    "window.zoomLevel": 0,
    "local-history.absolute": true,
    "local-history.path": "~/.vscode_history",
    "local-history.daysLimit": 10,
    "local-history.saveDelay": 20,
    "local-history.exclude": [
        "**/.history/**",
        "**/.vscode/**",
        "**/node_modules/**",
        "**/typings/**",
        "**/out/**",
        "**/Code/User/**"
    ]
}